{
  "name": "@atlas/api",
  "version": "1.0.0",
  "description": "NestJS Backend API with tRPC",
  "main": "dist/main.js",
  "exports": {
    ".": "./dist/main.js",
    "./@generated": {
      "types": "./src/@generated/server.ts"
    }
  },
  "typesVersions": {
    "*": {
      "@generated": [
        "./src/@generated/server.ts"
      ]
    }
  },
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@atlas/shared": "workspace:*",
    "@langchain/anthropic": "^1.3.12",
    "@langchain/community": "^1.1.8",
    "@langchain/core": "^1.1.17",
    "@langchain/textsplitters": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@nestjs/common": "^11.0.16",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.16",
    "@nestjs/platform-express": "^11.0.16",
    "@nestjs/throttler": "^6.2.2",
    "@supabase/supabase-js": "^2.47.0",
    "@trpc/server": "^11.0.0-rc.665",
    "@types/express": "^5.0.6",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.13.4",
    "axios-retry": "^4.5.0",
    "date-fns": "^4.1.0",
    "langchain": "^1.2.14",
    "nestjs-trpc": "^1.6.1",
    "openregister-mcp": "^2.2.0",
    "p-limit": "^7.2.0",
    "pdf-parse": "^1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@atlas/eslint-config": "workspace:*",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.0.16",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.12.8",
    "eslint": "^9.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.0"
  }
}
